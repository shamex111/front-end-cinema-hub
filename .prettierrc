{
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/api/(.*)$",
    "^@/app/(.*)$",
    "^@/assets/(.*)$",
    "^@/components/(.*)$",
    "^@/config/(.*)$",
    "^@/constants/(.*)$",
    "^@/hooks/(.*)$",
    "^@/services/(.*)$",
    "^@/types/(.*)$",
    "^@/utils/(.*)$",
    "^@/(.*)$",
    "^./(.*)$",
    "(.scss)$"
],
"importOrderSeparation": true,
"importOrderSortSpecifiers": true,
"plugins": ["@trivago/prettier-plugin-sort-imports"]
  }